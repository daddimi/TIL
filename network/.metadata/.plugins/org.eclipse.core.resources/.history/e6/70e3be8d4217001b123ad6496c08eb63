package com.sync;

public class Account {
	private int balance;
	public Account() {}
	public Account(int balance) {
		this.balance = balance;
	}
	
	public int getBalance() {
		return balance;
	}
	
	public void deposit(int money) {
		if(money > 0) {
			balance += money;
		}
	}
	
	public void withdraw(int money) {
		if(balance >= money) {
			balance -= money;
		}
	}
	
	public String toString() {
		return ;
	}

}
